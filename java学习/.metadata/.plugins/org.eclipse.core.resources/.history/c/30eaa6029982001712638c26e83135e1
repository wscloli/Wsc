package com.wsc.Throwable;

public class Text {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Text text=new Text();
		text.Text1();
	}
	public void Text1(){
	int a=12;
	int c = 3;
	int result = 0;
	try{
		while(c>-1){
			result=result+a/c;
		}
	}catch(Exception e){
		System.out.println("输出异常了");
		
	}finally {
		System.out.println("结果为"+result);
	}
}
}
