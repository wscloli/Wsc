package com.wsc.多态;

public class Rectangle extends Shape {
	public Rectangle(int a1,int b1){
		int a=a1;
		int b=b1;
	}
	@Override
	int c() {
		// TODO Auto-generated method stub
		int c=(a+b)*2;
		return c;
	}

	@Override
	int s() {
		// TODO Auto-generated method stub
		int s=a*b;
		return s;
	}

}
